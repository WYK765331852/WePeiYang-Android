<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/network_dialog_background"
    android:orientation="vertical">
    <EditText
        android:id="@+id/dialog_wifi_username_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_below="@id/dialog_wifi_vertify_iv"
        android:hint=""
        android:textSize="14sp"
        app:theme="@style/EditTextStyle"
        android:textCursorDrawable="@drawable/network_cursor_et"
        android:textColorHint="@color/net_hint_gray"
        />


</LinearLayout>